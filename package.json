{
  "name": "elm-webdriver",
  "version": "3.0.1",
  "description": "Elm bridge for Webdriver.io and testing utilities",
  "main": "index.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "directories": {
    "example": "example"
  },
  "bin": {
    "elm-webdriver": "bin/elm-webdriver.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorenzo/elm-webdriver.git"
  },
  "scripts": {
    "test": "./bin/elm-webdriver.js ./example/Main.elm",
    "dotTest": "./bin/elm-webdriver.js --dot ./example/Main.elm",
    "help": "./bin/elm-webdriver.js --help",
    "version": "./bin/elm-webdriver.js --version"
  },
  "files": [
    "bin",
    "src",
    "templates",
    "elm-package.json",
    "Readme.md",
    "Api.md"
  ],
  "keywords": [
    "elm",
    "elmlang",
    "webdriver",
    "cli",
    "test"
  ],
  "author": "José Lorenzo Rodríguez",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/lorenzo/elm-webdriver/issues"
  },
  "homepage": "https://github.com/lorenzo/elm-webdriver#readme",
  "dependencies": {
    "ascii-progress": "^1.0.2",
    "chalk": "^1.1.3",
    "chalkline": "0.0.5",
    "copy-dir": "^0.3.0",
    "cross-spawn": "^5.1.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-elm-compiler": "^4.3.1",
    "sanitize-filename": "^1.6.0",
    "temp": "^0.8.3",
    "uuid": "^3.0.0",
    "webdriverio": "^4.8.1"
  }
}
